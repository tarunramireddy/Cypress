{
    "name": "firstproject",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "cypress:run": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/mochawesome,overwrite=false,html=false,json=true",
        "cypress:merge": "mochawesome-merge cypress/reports/mochawesome/mochawesome_*.json > cypress/reports/mochawesome/tmp.json && move /Y cypress\\reports\\mochawesome\\tmp.json cypress\\reports\\mochawesome\\mochawesome.json",
        "cypress:report": "npx marge cypress/reports/mochawesome/mochawesome.json --reportDir . --reportFilename mochawesome --inline && start mochawesome.html",
        "test:report": "npm run cypress:run && npm run cypress:merge && npm run cypress:report"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "chai": "^5.2.0",
        "cypress": "^14.5.4",
        "cypress-mochawesome-reporter": "^3.8.2",
        "cypress-real-events": "^1.14.0",
        "cypress-xpath": "^2.0.1",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^5.0.0",
        "mochawesome-report-generator": "^6.2.0"
    }
}
